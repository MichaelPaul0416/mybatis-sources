<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wq.mybatis.source.demo.OfficeMapper">
    <resultMap id="queryAll" type="java.util.Map">
        <id column="officeCode" property="officeCode" jdbcType="VARCHAR" javaType="string"/>
        <result column="city" property="city" javaType="string" jdbcType="VARCHAR"/>
        <result column="phone" property="phone" javaType="string" jdbcType="VARCHAR"/>
        <result column="addressLine1" property="phone" javaType="string" jdbcType="VARCHAR"/>
        <result column="addressLine2" property="phone" javaType="string" jdbcType="VARCHAR"/>
        <result column="state" property="phone" javaType="string" jdbcType="VARCHAR"/>
        <result column="country" property="phone" javaType="string" jdbcType="VARCHAR"/>
        <result column="postalCode" property="phone" javaType="string" jdbcType="VARCHAR"/>
        <result column="territory" property="phone" javaType="string" jdbcType="VARCHAR"/>
    </resultMap>

    <select id="queryAllOffices" resultMap="queryAll">
        select * from offices
    </select>
</mapper>